def func(X):
    
    list = []
    dict = {}
    
    for i in range(len(X)):
        if X[i] not in dict:
            dict[X[i]] = str(X.count(X[i]))
            kv = str(X[i]) + str(dict[X[i]])
            list.append(kv)
    print dict
    print list
                                              
     
    return ''.join(list)
            
            
